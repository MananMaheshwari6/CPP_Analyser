{
  "name": "cpp-analyzer",
  "version": "1.0.0",
  "description": "A C++ code analyzer for time and space complexity",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd frontend && bun run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd frontend && bun install",
    "install-python-deps": "pip install pycparser",
    "test": "jest --coverage",
    "test:backend": "jest --testPathPattern=tests/backend",
    "test:python": "python -m pytest tests/python"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.6.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mongoose-memory-server": "^9.0.0"
  }
}
